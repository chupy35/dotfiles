   Xft.dpi: 168
   ! URxvt.font: -*-terminus-*-*-*-*-24-*-*-*-*-*-*-*
   ! URxvt.boldFont: -*-terminus-*-*-*-*-24-*-*-*-*-*-*-*
    URxvt.font: xft:ProFontWindows Nerd Font Mono:style=Regular:size=9
    xterm*font: xft:ProFontWindows Nerd Font Mono:style=Regular:size=9
    URxvt.boldFont: xft:ProFontWindows Nerd Font:style=Regular:size=9

    !Options
    URxvt.termName:rxvt-256color
    URxvt*urgentOnBell: true
    URxvt*visualBell: true
    URxvt*skipBuiltinGlyphs: true
    URxvt*termName: rxvt-unicode
    URxvt*borderless: 1
    URxvt*scrollBar: false
    URxvt*saveLines: 5000

    URxvt*utf8: 2
    URxvt.utf8: 2
    URxvt.transparent: true
    !URxvt.tintColor: #e8e8e8
    URxvt.shading: 35

    URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
    URxvt.url-select.launcher:  firefox
    URxvt.url-select.underline: true
    URxvt.keysym.M-u:           perl:url-select:select_next
    URxvt.keysym.M-Escape:      perl:keyboard-select:activate
    URxvt.keysym.M-s:           perl:keyboard-select:search
   
 !Links
    URxvt*perl-ext-common: matcher, font-size
    URxvt*urlLauncher: firefox
    URxvt*matcher.button: 1
    URxvt*matcher.pattern.1: \\bwww\\.[\\w-]+\\.[\\w./?&@URxvt-]*[\\w/-]

    !Cursor
    URxvt*cursorColor: #A36666
    URxvt*cursorBlink: true
   !!Font
   !URxvt.perl-lib:                     /home/chupy35/scripts/urxvt-font
   !URxvt.keysym.Control-Shift-Up:      perl:font:increment
   !URxvt.keysym.Control-Shift-Down:    perl:font:decrement
   !URxvt.perl-ext-common:              font



URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show


